package command;

public class Stereo {
	
	int volume;

	public void on() {
		System.out.println("Stereo is on");
	}

	public void off() {
		System.out.println("Stereo is off");
	}
	
	public void setCD(){
		System.out.println("CD is setting...");
	}
	
	public void setVolume(int volume){
		this.volume = volume;
		System.out.println("Volume is " + this.volume);
	}
	
}
