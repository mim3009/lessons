package factory;

public class CalifornyStyleVeggiePizza extends Pizza {

	public CalifornyStyleVeggiePizza() {
		name = "Californy Style Deep Dish Veggie Pizza";
		dough = "Extra Thick Crust Dough";
		sauce = "Plum Tomato Sauce";
		
		toppings.add("Shredded Mozzarella Cheese");
		toppings.add("Grated Reggiano Cheese");
	}
	
}
