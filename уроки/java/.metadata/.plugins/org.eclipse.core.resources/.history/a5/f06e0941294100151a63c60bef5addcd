package factory;

public class CalifornyStyleClamPizza extends Pizza {

	public CalifornyStyleClamPizza() {
		name = "Californy Style Deep Dish Clam Pizza";
		dough = "Extra Thick Crust Dough";
		sauce = "Plum Tomato Sauce";
		
		toppings.add("Shredded Mozzarella Cheese");
		toppings.add("Grated Reggiano Cheese");
	}
	
}
